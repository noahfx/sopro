  "description": "a SocietyPro application",
  "keywords": [],
  "dependencies": {
    "bower": "^1.3.12"
  },
  "scripts": {
    "bower": "bower install",
    "postinstall": "./node_modules/.bin/bower install",
    "prestart": "npm install",
    "pretest": "npm install",
    "test": "./node_modules/karma/bin/karma start ./tests/unit/karma.conf.js",
    "test-single-run": "./node_modules/karma/bin/karma start ./tests/unit/karma.conf.js  --single-run",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor tests/gui/protractor.conf.js",
    "mocha": "./node_modules/.bin/mocha --recursive -R xunit ./tests/unit/mocha/ > ./tests/unit/mocha-unit.xml",
    "newman": "./node_modules/.bin/newman -c tests/api_client/collections/testCollection.json.postman_collection --testReportFile tests/api_client/test-out.xml",
    "cucumber": "protractor tests/acceptance/protractor.conf.js"
  },
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/SocietyPro/sopro.git"
  },
  "author": "jimmymorales",
  "license": "",
  "devDependencies": {
    "cucumber": "^0.4.7",
    "cucumber-junit": "^1.1.10",
    "expect.js": "^0.3.1",
    "glob": "^4.3.5",
    "html5": "^1.0.5",
    "jasmine-core": "^2.2.0",
    "jasmine-reporters": "^1.0.1",
    "jasmine-spec-reporter": "^1.1.2",
    "karma": "^0.12.31",
    "karma-jasmine": "^0.3.5",
    "karma-junit-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "^0.1.4",
    "mocha": "^2.1.0",
    "newman": "^1.2.9",
    "phantomjs": "^1.9.15",
    "protractor-cucumber-junit": "^1.1.3",
    "should": "^5.0.0",
    "sinon": "^1.12.2",
    "zombie": "^2.5.1"
  }
}
